pipeline {
    agent any

    environment {
        SERVER_URL = 'http://localhost:8081'
    }

    stages {
        stage('Checkout') {
            steps {
                
                git branch: 'main', url: 'https://github.com/Pandidurai-22/Static-Website_New.git'
            }
        }

        stage('Build') {
            steps {
                // Run the build script
                sh './jenkins/script/build.sh'
            }
        }
    }
}